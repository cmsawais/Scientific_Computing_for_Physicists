    "# daxpy.py\n",
    "from typing import Sequence, List\n",
    "def daxpy(a, x, y):\n",
    "    if len(x) != len(y):\n",
    "        raise ValueError(\"x and y must have the same length\")\n",
    "    return [a*xi + yi for xi, yi in zip(x, y)]\n"
   ]
  }
